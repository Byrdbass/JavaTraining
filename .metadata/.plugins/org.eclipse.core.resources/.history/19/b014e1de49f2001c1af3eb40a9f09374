
public class LogicalOperators {
	public static void main(String[] args) {
		
		int numOne= 100;
		int numTwo = 20;
		int numThree = 30;
		
		// && -> AND , || -> OR , ! -> NOT
		
		System.out.println(numOne>numTwo && numOne>numThree);
		
		System.out.println(numOne>numTwo || numOne>numThree);
		
	}
}
