
public class QuizDataTypes {
	public static void main(String[] args) {
		int time = 5;
		float rate = 10f;
		double principal = 5000;
		System.out.println((principal*rate*time)/100);
		
		int time2 = 3;
		float rate2 = 7f;
		double principal2 = 3250;
		System.out.println((principal2*rate2*time2)/100);
	}
}
